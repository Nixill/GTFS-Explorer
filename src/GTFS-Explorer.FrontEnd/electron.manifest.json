{
  "executable": "GTFS-Explorer",
  "splashscreen": {
    "imageFile": ""
  },
  "name": "GTFS-Explorer",
  "author": "",
  "singleInstance": false,
  "environment": "Production",
  "build": {
    "appId": "com.GTFS-Explorer.app",
    "productName": "GTFS-Explorer",
    "copyright": "Copyright Â© 2021",
    "buildVersion": "0.1.2",
    "compression": "maximum",
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [
          "**/*"
        ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [
          "**/*"
        ]
      },
      "**/*"
    ]
  }
}